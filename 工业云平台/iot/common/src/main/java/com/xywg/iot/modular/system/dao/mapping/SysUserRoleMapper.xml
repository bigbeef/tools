<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xywg.iot.modular.system.dao.SysUserRoleMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <insert id="insertUserRole" parameterType="com.xywg.iot.modular.system.model.SysUserRole">
        INSERT INTO
        sys_user_role(id,user_id,role_id,del_flag)
        VALUES (#{id},#{userId},#{roleId},#{delFlag})
    </insert>
</mapper>
